{
    "manifest_version": 2,
    "name": "Jira Standupizer",
    "description": "Decides who's next on standups",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://dev-jira.dynatrace.org/secure/RapidBoard.jspa"],
        "js": ["main.js"]
      }
    ],

    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ]
  }